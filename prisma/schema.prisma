datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
  interface            = "sync"
}

model Doctors {
  id        Int     @id @default(autoincrement())
  nom	String
  genre	String
  specialite	String
  address_dsp	String
  daira	String
  telephone	String
  lat	String
  lng String
}